{
  "presets": ["@babel/env"],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-object-rest-spread",
    "wildcard",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@": "./src",
        "Routes": "./src/routes",
        "Controllers": "./src/controllers",
        "Services": "./src/services",
        "Utils": "./src/utils"
      }
    }]
  ]
}
